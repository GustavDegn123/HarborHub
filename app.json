{
  "expo": {
    "name": "HarborHub",
    "slug": "harborhub",
    "scheme": "harborhub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.gustavdegn.harborhub",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "HarborHub bruger din placering til at s√¶tte opgavens position.",
        "UIBackgroundModes": [
          "remote-notification",
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.gustavdegn.harborhub",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "POST_NOTIFICATIONS"
      ]
    },
    "notification": {
      "icon": "./assets/logo.png",
      "color": "#0B6EEF"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "extra": {
      "expoClientId": "16622525056-qcgjdv8gkbunfgv4c8g79qm0brnjvoj5.apps.googleusercontent.com",
      "iosClientId": "16622525056-7pgliodrdtnruh16cobp7kjb8h838g58.apps.googleusercontent.com",
      "androidClientId": "16622525056-3j94ogkp9q6gs2bn0p1q2q50iv9dt0q1.apps.googleusercontent.com",
      "webClientId": "16622525056-i5cbljlogf92qbdc505gcbrn8cne8r48.apps.googleusercontent.com",
      "eas": {
        "projectId": "02ce5b94-a91d-415e-9f67-20d7006f114c"
      }
    },
    "plugins": [
      "expo-notifications",
      [
        "@criipto/verify-expo",
        {
          "androidAppLinks": []
        }
      ]
    ],
    "owner": "gustavdegn"
  }
}
